# $coat_image
!! get the image for coats
!! ARGS 0 - coats group name
!! ARGS 1 - coats index

if $ARGS[0] = 'moncheri':
	$RESULT = 'images/pc/items/moncheri/coats/<<ARGS[1]>>.jpg'
elseif $ARGS[0] = 'bomba':
	$RESULT = 'images/pc/items/bomba/coats/<<ARGS[1]>>.jpg'
elseif $ARGS[0] = 'cats':
	$RESULT = 'images/pc/items/cats/coats/<<ARGS[1]>>.jpg'
elseif $ARGS[0] = 'dolls':
	$RESULT = 'images/pc/items/dolls/coats/<<ARGS[1]>>.jpg'
elseif $ARGS[0] = 'gm':
	$RESULT = 'images/pc/items/gm/coats/<<ARGS[1]>>.jpg'
elseif $ARGS[0] = 'danilovich':
	$RESULT = 'images/pc/items/danilovich/coats/<<ARGS[1]>>.jpg'
end

