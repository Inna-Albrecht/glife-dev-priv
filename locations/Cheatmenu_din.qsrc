	pl'hypnoTime: <<hypnoTime>>'
	pl'hypnoStrength: <<hypnoStrength>>'
	pl'hypnoDay: <<hypnoDay>>'
	pl'hypnoAddict: <<hypnoAddict>>'
	pl'missCum: <<missCum>>'
	pl'condoms/bad condoms: <<prezik>>/<<badprezik>>'
	if tabletki > 0:
		pl'pack(s) of birth control pills: <<tabletki>>'
		if tabletkiold > 0: pl'   <<tabletkiold>> expired pack'
		if tabletkifake > 0: pl'   <<tabletkifake>> counterfeit pack'
		if tabletkirej > 0: pl'   <<tabletkirej>> factory rejected pack'
		if tabletkifert > 0: pl'   <<tabletkifert>> fertility hormones pack'
		if tabletkisug > 0: pl'   <<tabletkisug>> sugar pills pack'
