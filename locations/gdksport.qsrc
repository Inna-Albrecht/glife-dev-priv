if $ARGS[0] = 'volley':
	menu_off = 1
	gs 'exercise', 'tier2', 30, 'vball_exp', 'run_exp', 'agil_exp', 'stren_exp', 'vital_exp'
!	minut += 30
	grupvalue[2] += 1
	gs 'stat'

	if volltime = 0:
		!! reset the scores
		volpoint = 0
		volpointv = 0

	'The teams take to the court and start playing.'

		!! reserve player or you were caught drinking by the coach and he forbid you from playing
		if trener_Speak < 8 or seealko = 1:
			volleysostav = 1
			poverkomand = 50
			'<center><img <<$set_imgh>> src="images/locations/pavlovsk/community/gym/volley/game3.jpg"></center>'
			'You sit on the bench watching your team play.'

		!! starter
		else
			volleysostav = 2
			grupvalue[2] += 1
			!! between 0 and 200 dependent on player skills
			poverkomand = ((pcs_vball*2) + ((pcs_stren + pcs_vital + pcs_agil + pcs_react)/2))/2
			'<center><img <<$set_imgh>> src="images/locations/pavlovsk/community/gym/volley/game4.jpg"></center>'
			'After warming up you run out on the court to join your teammates.'
		end

		!! opponent skill - between 0 and 200. A perfect player should be able to win every match.
		poverkomandV = rand(0, 200)

		if poverkomand > poverkomandV:
			volpoint = 1
			'Your team plays better than your opponents.'
		elseif poverkomand < poverkomandV:
			volpointv = 1
			'Your opponents play better than your team.'
		else
			'The scoring varies one way, then the other, obviously the teams are evenly matched.'
		end

		act 'Continue':volltime = 1 & gt 'gdksport', 'volley'

	!! halftime
	elseif volltime = 1:
		'During the break, both teams have a rest.'

		if volpointv = 0:
			'<center><img <<$set_imgh>> src="images/characters/pavlovsk/school/teacher/mikhail/volleytrener6.jpg"></center>'
			'Coach tells the team that you can''t relax. "Although we''re winning, you still need to be focused! Don''t let the enemy back into the game!"'
		elseif volpoint = 0:
			'<center><img <<$set_imgh>> src="images/characters/pavlovsk/school/teacher/mikhail/volleytrener2.jpg"></center>'
			'Coach tells the team that losing the first half is bad, but it''s not a disaster. "You pick yourselves up and go out and kick their asses. You can still win this game in the second half."'
		else
			'<center><img <<$set_imgh>> src="images/characters/pavlovsk/school/teacher/mikhail/volleytrener1.jpg"></center>'
			'Coach tells the team that there is no draws in volleyball, he expects you to keep doing the things that you are doing well, but increase pressure. "With more discipline and toughness, you can win this game!"'
		end

		if volleysostav = 1:
			'You are sitting on the bench.'

			act 'Ask to play':
				if seealko = 1:
					*clr & cla
					'The coach refuses to let you play because he caught you drinking.'
					act 'Continue':volltime = 2 & gt 'gdksport', 'volley'
				else
					*clr & cla
					gs 'stat'

					if npc_rel['A69'] >= rand(0, 80):
						volleysostav = 2
						poverkomand = ((pcs_vball*2) + ((pcs_stren + pcs_vital + pcs_agil + pcs_react)/2))/2
						'The coach agrees to let you play the second half.'

						act 'Continue':volltime = 2 & gt 'gdksport', 'volley'
					else
						'You ask the coach to play, but he shakes his head and explains that it is not strategically right for the team.'

						act 'Continue':volltime = 2 & gt 'gdksport', 'volley'
					end
				end
			end
		end

		act 'Continue':volltime = 2 & gt 'gdksport', 'volley'

	!! 2nd half
	elseif volltime = 2:
		*clr & cla
		'<center><img <<$set_imgh>> src="images/locations/pavlovsk/community/gym/volley/game5.jpg"></center>'
		'Break is over and the team starts the second half.'

		if volleysostav = 1:'You sit on the bench watching your team play.'


		!! change the opponent skill a bit so both halves are not an entirely static affair.
		poverkomandV += rand(-50, 50)

		if poverkomand > poverkomandV:
			volpoint += 1
			'Your team plays better than your opponents.'

			!! you came in and your team scored when they would not have without you
			if volleysostav = 2 and volpointv = 1:npc_rel['A69'] += 10
		elseif poverkomand < poverkomandV:
			volpointv += 1
			'Your opponents play better than your team.'

			!! you came in and your team gave up a point they would not have without you
			if volleysostav = 2 and volpoint = 1:npc_rel['A69'] -= 5
		else
			'The scoring varies one way, then the other. The teams are obviously evenly matched.'

			!! you came in and your team tied when they would have lost without you
			if volleysostav = 2 and volpointv = 1:npc_rel['A69'] += 5
		end

		act 'Continue':volltime = 3 & gt 'gdksport', 'volley'

	!! end of game
	elseif volltime = 3:
		*clr
		cla
		volsesongame += 1
		if rand (1, 100) < 3:
			if volpointv = 2:
				volpoint = 0
				volpointv = 2
			elseif volpoint = 2:
				volpoint = 2
				volpointv = 0
			end
		end

		'Final whistle.'
		seealko = 0
		if volpoint > volpointv:
			volsesonwin += 1
			grupvalue[2] += 1
			'<center><img <<$set_imgh>> src="images/locations/pavlovsk/community/gym/volley/game6.jpg"></center>'
			'Your team wins.'
		elseif volpoint < volpointv:
			volsesonloss += 1
			'<center><img <<$set_imgh>> src="images/locations/pavlovsk/community/gym/volley/game8.jpg"></center>'
			'Your team loses.'
		else
			if rand (0, 1) = 0:
				volsesonwin += 1
				grupvalue[2] += 1
				'<center><img <<$set_imgh>> src="images/locations/pavlovsk/community/gym/volley/game7.jpg"></center>'
				'The match was very close, but in the end your team pulls off a win.'
			else
				volsesonloss += 1
				'<center><img <<$set_imgh>> src="images/locations/pavlovsk/community/gym/volley/game7.jpg"></center>'
				'The match was very close, but in the end your team still loses.'
