			!!	energy usage flag on.
			energytomilkpump = 1
			pcs_sweat += pumptime/30
		else
			pcs_stam -= lactation['max_sucktion_demand']/30000
			pcs_sweat += pumptime/30
			!!	energy usage flag on.
			energytomilkpump = 1
		end
