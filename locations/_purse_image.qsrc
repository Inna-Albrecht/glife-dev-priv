# $purse_image
!! get the image for purses
!! ARGS 0 - panty shop name
!! ARGS 1 - panty index

if $ARGS[0] = 'dolls':
	$RESULT = 'images/pc/items/dolls/purses/<<ARGS[1]>>.jpg'
elseif $ARGS[0] = 'fashionista':
	$RESULT = 'images/pc/items/fashionista/purses/<<ARGS[1]>>.jpg'
elseif $ARGS[0] = 'coco':
	$RESULT = 'images/pc/items/coco/purses/<<ARGS[1]>>.jpg'
elseif $ARGS[0] = 'flamingos':
	$RESULT = 'images/pc/items/flamingos/purses/<<ARGS[1]>>.jpg'
elseif $ARGS[0] = 'danilovich':
	$RESULT = 'images/pc/items/danilovich/purses/<<ARGS[1]>>.jpg'
elseif $ARGS[0] = 'gm':
	$RESULT = 'images/pc/items/gm/purses/<<ARGS[1]>>.jpg'
elseif $ARGS[0] = 'cats':
	$RESULT = 'images/pc/items/cats/purses/<<ARGS[1]>>.jpg'
elseif $ARGS[0] = 'moncheri':
	$RESULT = 'images/pc/items/moncheri/purses/<<ARGS[1]>>.jpg'
end

