			locat['Kolka'] = 1
			locat['Kolka_athome'] = 0
		end
	else
		if hour < 6 or (hour = 6 and minut < 45):
			if brothermorningsex ! daystart:
				!! Sleep
				locat['Kolka'] = 1
				locat['Kolka_athome'] = 0
			else
				!! Awake - Sveta woke him up
				locat['Kolka'] = 2
				locat['Kolka_athome'] = 1
			end
		elseif (hour = 6 and minut >= 45) or (hour = 7 and minut <= 15):
			!! Family Breakfast
			locat['Kolka'] = 3
			locat['Kolka_athome'] = 1
		elseif hour < 10:
			if kanikuli = 0 and brother_homework ! daystart:
				!! Homework
				locat['Kolka'] = 7
				locat['Kolka_athome'] = 1
			else
				!! Watches TV or games
				locat['Kolka'] = 11
				locat['Kolka_athome'] = 1
			end
		elseif hour <= 13:
			!! Watches TV
			locat['Kolka'] = 11
			locat['Kolka_athome'] = 1
		elseif hour < 18:
			!! Leaves apartment
			locat['Kolka'] = 9
			locat['Kolka_athome'] = 0
		elseif hour = 18 and minut <= 30:
			!! Family Dinner
			locat['Kolka'] = 8
			locat['Kolka_athome'] = 1
		elseif hour < 21:
			!! Watches TV or games
			locat['Kolka'] = 11
			locat['Kolka_athome'] = 1
		elseif hour = 21 and minut < 15:
			!! Shower
			locat['Kolka'] = 10
			locat['Kolka_athome'] = 1
		elseif hour < 23:
			!! Watches TV or games
			locat['Kolka'] = 11
			locat['Kolka_athome'] = 1
		else
			!! Sleep
			locat['Kolka'] = 1
			locat['Kolka_athome'] = 0
		end
	end
end

