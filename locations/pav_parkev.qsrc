	 !lightweight
		player_time = rand(player_tol,99) - 29
	else 
		player_tol += 29
		player_time = rand(player_tol, 99)
	end
	
	if player_time < 0: player_time = 0

    if lena_time < lera_time:
        high1 = lera_time
		$g_win = 'Lera'
    else 
        high1 = lena_time
		$g_win = 'lena'
	end
	
    if alyona_time < nush_time:
        high2 = nush_time
		$g_win2 = 'nush'
