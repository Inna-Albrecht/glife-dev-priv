		gt 'gschool_lunch', 'break'
	elseif school_period = 3:
		school_period = 4
		if hour = 9 and minut < 55: minut += 55 - minut
		if week = 1: gt 'gschool_lessons', 'literature'
		if week = 2: gt 'gschool_lessons', 'science'
		if week = 3: gt 'gschool_lessons', 'biology'
		if week = 4: gt 'gschool_lessons', 'science'
		if week = 5: gt 'gschool_lessons', 'literature'
	elseif school_period = 4:
		school_period = 5
		if hour = 10 and minut < 45: minut += 45 - minut
		if week = 1: gt 'gschool_lessons', 'art'
		if week = 2: gt 'gschool_lessons', 'shop'
		if week = 3: gt 'gschool_lessons', 'history'
		if week = 4: gt 'gschool_lessons', 'shop'
		if week = 5: gt 'gschool_lessons', 'art'
	elseif school_period = 5:
		school_period = 6
		gt 'gschool_lunch', 'lunch'
	elseif school_period = 6:
		school_period = 7
		if hour = 12 and minut < 25: minut += 25 - minut
		if week = 1: gt 'gschool_lessons', 'biology'
		if week = 2: gt 'gschool_lessons', 'computer'
		if week = 3: gt 'gschool_lessons', 'computer'
		if week = 4: gt 'gschool_lessons', 'computer'
		if week = 5: gt 'gschool_lessons', 'history'
	elseif school_period = 7:
		school_period = 8
		if hour = 13 and minut < 15: minut += 15 - minut
		if week = 1: gt 'gschool_lessons', 'pe'
		if week = 2: gt 'gschool_lessons', 'music'
		if week = 3: gt 'gschool_lessons', 'pe'
		if week = 4: gt 'gschool_lessons', 'music'
		if week = 5: gt 'gschool_lessons', 'pe'
	else
		school_period = 0
		gt 'gschool_grounds', 'main'
	end
